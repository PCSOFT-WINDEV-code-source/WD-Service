   -
     code : |1-
      
      procedure FEN_Install(ModeManuel = faux)
      
      // Mode manuel (pour installation ou désinstallation)
      si ModeManuel ALORS
      	
      	// Vérifie si le service existe
      	si ServiceExiste(service_nom) alors
      		
      		// Propose de désinstaller le service
      		LIB_INFOSERVICE = "Le service est installé."
      		BTN_INSTALL..libelle = "Dés&installer"
      	sinon
      		
      		// Propose d'installer le service
      		LIB_INFOSERVICE = "Le service n'est pas installé."
      		BTN_INSTALL..Libellé = "&Installer"			
      	FIN
      	
      	// Plan 2	
      	FEN_Install..plan = 2
      sinon
      	
      	// Plan par défaut
      	FEN_Install..Plan = 1
      FIN
